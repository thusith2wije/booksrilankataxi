}

/* Responsive design */
@media (max-width: 1024px) {
  .cards-wrapper {
    gap: 15px; /* Increase spacing slightly */
  }
}

@media (max-width: 768px) {
  .cards-wrapper {
    flex-direction: column;
    align-items: center;
  }

  .uc_content_box_zoom_effect {
    max-width: 300px;
    height: auto;
  }
}

