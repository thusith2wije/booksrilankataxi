    <script>
      document.addEventListener("DOMContentLoaded", function () {
        // Initialize phone input
        const input = document.querySelector("#whatsapp-number");
        const iti = window.intlTelInput(input, {
          initialCountry: "lk",
          preferredCountries: ["lk", "us", "gb"],
          separateDialCode: true,
          utilsScript:
            "https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.19/js/utils.js",
        });

        let lastFailureTime = 0;

        // Form submission
        document
          .querySelector(".form-box")
          .addEventListener("submit", function (event) {
            event.preventDefault();

            const name = document
              .querySelector('input[name="Name"]')
              .value.trim();
            const email = document
              .querySelector('input[name="Email"]')
              .value.trim();
            const whatsapp = iti.getNumber();
            const subject = document
              .querySelector('textarea[name="Subject"]')
              .value.trim();
            const message = document
              .querySelector('textarea[name="Message"]')
              .value.trim();

            console.log(!name, !email, !whatsapp, !subject, !message);

            if (!name || !email || !whatsapp || !subject || !message) {
              alert("Please fill in all fields!");
              return;
            }

            const requestData = {
              name,
              from: email,
              phoneNo: whatsapp,
              subject,
              message,
            };

            fetch(
              "https://api.staging.booksrilankataxi.com/messenger/email/contact-us",
              {
                method: "POST",
                headers: { "Content-Type": "application/json" },
                body: JSON.stringify(requestData),
              }
            )
              .then((response) => response.json())
              .then((result) => {
                if (result.success) {
                  alert("Your message has been sent successfully!");
                  event.target.reset(); // ✅ Reset form only after success
                } else {
                  alert("Error: " + result.message);
                }
              })
              .catch((error) => {
                handleFailure(
                  "Failed to send message. Please try again later."
                );
              });

            function handleFailure(errorMessage) {
              const currentTime = new Date().getTime();
              if (
                lastFailureTime === 0 ||
                currentTime - lastFailureTime > 10000
              ) {
                alert(errorMessage);
              } else {
                alert(
                  "We are experiencing issues sending your message. Please try again later."
                );
              }
              lastFailureTime = currentTime;
            }
          });
      });
    </script>

